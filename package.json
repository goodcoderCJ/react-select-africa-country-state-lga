{
  "name": "react-select-africa-country-state-lga",
  "version": "1.0.4",
  "description": "A React Select library for the Regions of Africa to get the Countries, States or Regions or Province, LGA's of the selected ",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siccoo/react-select-africa-country-state-lga.git"
  },
  "keywords": [
    "Africa",
    "Region",
    "Country",
    "State",
    "Province",
    "Local",
    "Government",
    "Area",
    "LGA",
    "React-component",
    "Capital"
  ],
  "files": [
    "dist",
    "LICENSE",
    "ReadME.md"
  ],
  "author": "Chilaka Michael Obinna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siccoo/react-select-africa-country-state-lga/issues"
  },
  "homepage": "https://github.com/siccoo/react-select-africa-country-state-lga#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select-africa-country-state-lga": "^1.0.4",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.4"
  },
